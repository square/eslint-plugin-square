{
  "name": "eslint-plugin-square",
  "version": "13.3.0",
  "description": "Linting plugin containing Square's custom lint rules and configuration for JavaScript and related technologies.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel lint:*",
    "lint:docs": "markdownlint '**/*.md' -i CHANGELOG.md -i node_modules",
    "lint:js": "eslint .",
    "preversion": "yarn test && yarn lint",
    "test": "nyc --all --check-coverage mocha --recursive tests"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ember",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "linter",
    "react",
    "typescript"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/square/eslint-plugin-square.git"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "camelcase": "^6.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^8.4.0",
    "eslint-plugin-es": "^3.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "espree": "^6.2.1",
    "estraverse": "^5.1.0",
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "eslint": "6.8.0",
    "eslint-plugin-eslint-plugin": "2.2.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "19.0.1",
    "lerna-changelog": "1.0.1",
    "markdownlint-cli": "0.22.0",
    "mocha": "7.1.2",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.1",
    "strip-indent": "3.0.0"
  },
  "nyc": {
    "branches": 93,
    "lines": 96,
    "functions": 97,
    "statements": 96
  }
}
