{
  "name": "eslint-plugin-square",
  "version": "23.1.1",
  "description": "Linting plugin containing Square's custom lint rules and configuration for JavaScript and related technologies.",
  "keywords": [
    "ember",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintplugin",
    "linter",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/square/eslint-plugin-square.git"
  },
  "license": "Apache-2.0",
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "files": [
    "dist/lib/index.d.ts",
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel lint:*",
    "lint:docs": "markdownlint \"**/*.md\"",
    "lint:eslint-docs": "npm-run-all \"update:eslint-docs -- --check\"",
    "lint:js": "eslint --cache .",
    "lint:package-json": "npmPkgJsonLint .",
    "lint:package-json-sorting": "sort-package-json --check",
    "lint:package-json-sorting:fix": "sort-package-json package.json",
    "lint:types": "tsc",
    "prepublishOnly": "tsc",
    "preversion": "yarn test && yarn lint",
    "release": "release-it",
    "test": "nyc --all --check-coverage mocha --recursive tests",
    "update:eslint-docs": "eslint-doc-generator --config-emoji ember,ðŸ”¥ --url-configs \"https://github.com/square/eslint-plugin-square/blob/master/README.md#configurations\""
  },
  "prettier": "@square/prettier-config",
  "nyc": {
    "branches": 93,
    "functions": 100,
    "lines": 98,
    "statements": 98
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-ember": "^11.0.6",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-utils": "^3.0.0",
    "espree": "^9.3.3",
    "prettier": "^2.7.1",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "@square/prettier-config": "^2.0.0",
    "@types/eslint": "^8.4.6",
    "@types/eslint-utils": "^3.0.2",
    "@types/estree": "^1.0.0",
    "@types/jest": "^29.1.1",
    "@types/requireindex": "^1.2.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.24.0",
    "eslint-doc-generator": "^0.16.0",
    "eslint-plugin-eslint-plugin": "^5.0.6",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-square": "file:./",
    "markdownlint-cli": "^0.32.2",
    "mocha": "^10.0.0",
    "npm-package-json-lint": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "release-it": "^15.5.0",
    "sort-package-json": "^2.0.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "eslint": ">= 8.18.0"
  },
  "engines": {
    "node": "^14.18.0 || ^16.0.0 || >= 18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
