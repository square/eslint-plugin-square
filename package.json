{
  "name": "eslint-plugin-square",
  "version": "18.0.0",
  "description": "Linting plugin containing Square's custom lint rules and configuration for JavaScript and related technologies.",
  "keywords": [
    "ember",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "linter",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/square/eslint-plugin-square.git"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel lint:*",
    "lint:docs": "markdownlint '**/*.md'",
    "lint:js": "eslint --cache .",
    "lint:package-json": "sort-package-json --check",
    "release": "release-it",
    "test": "nyc --all --check-coverage mocha --recursive tests",
    "preversion": "yarn test && yarn lint"
  },
  "prettier": "@square/prettier-config",
  "nyc": {
    "branches": 93,
    "functions": 97,
    "lines": 96,
    "statements": 96
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "camelcase": "^6.2.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-ember": "^10.2.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-qunit": "^6.0.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-unicorn": "^29.0.0",
    "espree": "^7.3.0",
    "estraverse": "^5.2.0",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@square/prettier-config": "^1.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-eslint-plugin": "^2.3.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "markdownlint-cli": "^0.27.1",
    "mocha": "^8.3.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "release-it": "^14.5.0",
    "release-it-lerna-changelog": "^3.1.0",
    "sort-package-json": "^1.49.0",
    "strip-indent": "^3.0.0"
  },
  "peerDependencies": {
    "eslint": ">= 6"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
