{
  "name": "eslint-plugin-square",
  "version": "15.1.1",
  "description": "Linting plugin containing Square's custom lint rules and configuration for JavaScript and related technologies.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel lint:*",
    "lint:docs": "markdownlint '**/*.md'",
    "lint:js": "eslint --cache .",
    "lint:package-json": "npmPkgJsonLint .",
    "preversion": "yarn test && yarn lint",
    "test": "nyc --all --check-coverage mocha --recursive tests"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "ember",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "linter",
    "react",
    "typescript"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/square/eslint-plugin-square.git"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14.*"
  },
  "peerDependencies": {
    "eslint": ">= 6"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "camelcase": "^6.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-es": "^3.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-unicorn": "20.1.0",
    "espree": "^7.0.0",
    "estraverse": "^5.1.0",
    "npm-package-json-lint": "^5.1.0",
    "prettier": "^2.0.5"
  },
  "devDependencies": {
    "@square/prettier-config": "^1.0.0",
    "eslint": "7.4.0",
    "eslint-plugin-eslint-plugin": "2.3.0",
    "eslint-plugin-node": "11.1.0",
    "lerna-changelog": "1.0.1",
    "markdownlint-cli": "0.23.2",
    "mocha": "8.0.1",
    "npm-run-all": "4.1.5",
    "nyc": "15.1.0",
    "strip-indent": "3.0.0"
  },
  "nyc": {
    "branches": 93,
    "lines": 96,
    "functions": 97,
    "statements": 96
  },
  "prettier": "@square/prettier-config"
}
