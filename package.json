{
  "name": "eslint-plugin-square",
  "version": "22.1.0",
  "description": "Linting plugin containing Square's custom lint rules and configuration for JavaScript and related technologies.",
  "keywords": [
    "ember",
    "eslint",
    "eslint-config",
    "eslint-plugin",
    "eslintplugin",
    "linter",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/square/eslint-plugin-square.git"
  },
  "license": "Apache-2.0",
  "exports": "./lib/index.js",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "npm-run-all --continue-on-error --aggregate-output --parallel lint:*",
    "lint:docs": "markdownlint \"**/*.md\"",
    "lint:js": "eslint --cache .",
    "lint:package-json": "npmPkgJsonLint .",
    "lint:package-json-sorting": "sort-package-json --check",
    "lint:package-json-sorting:fix": "sort-package-json package.json",
    "preversion": "yarn test && yarn lint",
    "release": "release-it",
    "test": "nyc --all --check-coverage mocha --recursive tests"
  },
  "prettier": "@square/prettier-config",
  "nyc": {
    "branches": 93,
    "functions": 100,
    "lines": 98,
    "statements": 98
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "eslint-utils": "^3.0.0",
    "espree": "^9.3.1",
    "prettier": "^2.5.1",
    "requireindex": "^1.2.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@square/prettier-config": "^1.0.0",
    "eslint": "^8.10.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-square": "file:./",
    "markdownlint-cli": "^0.31.1",
    "mocha": "^9.2.1",
    "npm-package-json-lint": "^5.4.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "release-it": "^14.12.5",
    "release-it-lerna-changelog": "^4.0.1",
    "sort-package-json": "^1.54.0"
  },
  "peerDependencies": {
    "eslint": ">= 8.8.0"
  },
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
